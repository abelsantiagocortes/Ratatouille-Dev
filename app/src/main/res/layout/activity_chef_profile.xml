<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Chef.ChefProfile">

    <TextView
        android:id="@+id/txt_sub_forgot5"
        android:layout_width="206dp"
        android:layout_height="39dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="164dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/raleway_extrabold"
        android:inputType="textPersonName"
        android:text="@string/app_name"
        android:textAlignment="viewStart"
        android:textColor="#E2000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nameChef"
        style="@android:style/Widget.DeviceDefault.Light.TextView"
        android:layout_width="147dp"
        android:layout_height="141dp"
        android:layout_marginStart="224dp"
        android:layout_marginBottom="72dp"
        android:font="@font/raleway_extrabold"
        android:fontFamily="@font/raleway_extrabold"
        android:gravity="center|center_horizontal|center_vertical"
        android:paddingStart="10dp"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nameChef2"

        android:layout_width="310dp"
        android:layout_height="111dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="79dp"
        android:layout_marginBottom="56dp"
        android:gravity="center|center_horizontal|center_vertical"
        android:paddingStart="10dp"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nameChef3"

        android:layout_width="311dp"
        android:layout_height="149dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="16dp"
        android:gravity="center|center_horizontal|center_vertical"
        android:paddingStart="10dp"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_volver"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_volver"
        style="@style/btn_txt_white_background_grey"
        android:layout_width="189dp"
        android:layout_height="65dp"
        android:layout_marginBottom="28dp"
        android:text="Solicitar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="173dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="4dp"
        android:font="@font/raleway_extrabold"
        android:fontFamily="@font/raleway_extrabold"
        android:text="EXPERIENCIA"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/nameChef2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="173dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="12dp"
        android:font="@font/raleway_extrabold"
        android:fontFamily="@font/raleway_extrabold"
        android:text="CERTIFICADOS"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/nameChef3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.815"
        app:layout_constraintStart_toStartOf="parent" />

    <com.github.siyamed.shapeimageview.CircularImageView
        android:id="@+id/chefin"
        android:layout_width="202dp"
        android:layout_height="212dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/divider"
        android:layout_width="391dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <View
        android:id="@+id/divider2"
        android:layout_width="391dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>